@model IEnumerable<NWHarvest.Web.ViewModels.AdministratorListingViewModel>
@{ 
    TempData["NotOrderableColumns"] = new int[] { 5 };
}
<table class="table table-hover table-responsive" id="listingTable">
    <thead>
        <tr>
            <th>Grower</th>
            <th>Product</th>
            <th>Available</th>
            <th>Claimed</th>
            <th>Expires</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Lister.Name.Trim()</td>
            <td>@item.Product</td>
            <td>@item.AvailableQuantity</td>
            <td>@item.ClaimedQuantity</td>
            <td>@item.ExpirationDate.ToShortDateString()</td>
            <td>@Html.ActionLink("Delete", "Delete", "Listings", new { Id = item.Id, ReturnUrl = ViewBag.ReturnUrl }, null)</td>
        </tr>
        }
    </tbody>
</table>